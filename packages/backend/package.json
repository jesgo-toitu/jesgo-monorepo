{
    "name": "jesgo-server",
    "version": "1.5",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "moduleNameMapper": {
            "^#/(.+)": "<rootDir>/backendapp/$1"
        },
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/tests/**/*.test.ts"
        ]
    },
    "config": {
        "configPath": {
            "development": "./backendapp/config/config.json",
            "production": "../config/config.json"
        }
    },
    "scripts": {
        "start": "cmd /c \"if defined PATH_NODE (\"\"%PATH_NODE%\"\" ./backendapp/app.js) else (node ./backendapp/app.js)\"",
        "dev-start": "node ./dist/app.js",
        "dev-watch": "nodemon --watch backendapp --ext ts --exec \"ts-node backendapp/app.ts\"",
        "dev-live": "ts-node backendapp/app.ts",
        "prod-start": "cross-env NODE_ENV=production node ./dist/app.js",
        "build": "tsc",
        "lint": "eslint --ext .tsx,.ts backendapp/",
        "fmt": "prettier --write backendapp/**/*",
        "test": "jest",
        "install-service": "winser -i",
        "uninstall-service": "winser -r -x",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@jesgo/common": "file:../common",
        "acorn": "^8.8.2",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "cors": "^2.8.5",
        "esm": "^3.2.25",
        "express": "^4.17.2",
        "express-promise-router": "^4.1.1",
        "fs-extra": "^10.1.0",
        "helmet": "^5.0.1",
        "iconv-lite": "^0.6.3",
        "jsonpointer": "^5.0.1",
        "jsonwebtoken": "^8.5.1",
        "log4js": "^6.5.1",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "patch-package": "^6.4.7",
        "pg": "^8.7.1",
        "qs": "^6.10.5",
        "unzipper": "^0.10.14",
        "uuidjs": "^4.2.12",
        "winser": "^1.0.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.0",
        "@types/json-pointer": "^1.0.31",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/lodash": "^4.14.181",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.10",
        "@types/pg": "^8.6.4",
        "@types/unzipper": "^0.10.5",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-react": "^7.29.2",
        "jest": "^27.4.7",
        "nodemon": "^2.0.20",
        "prettier": "2.5.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.4"
    }
}
